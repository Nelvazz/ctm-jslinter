{
    "rules": {
        "noVar": true,
        "noConsoleLog": false,
        "constantCaps": true,
        "enforceSemicolon": true,
        "consistentIndentation": 4,
        "noTrailingWhitespace": true,
        "preferConst": true,
        "noEmptyBlocks": true,
        "useTripleEquals": true,
        "spaceBetweenOperators": true,
        "fileNamingConvention": true,
        "maxLinesPerFunction": 25
    },
    "ignore": ["node_modules"]
}